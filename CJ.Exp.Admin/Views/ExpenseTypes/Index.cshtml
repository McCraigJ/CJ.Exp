@model ExpenseTypesVM
@{
  ViewData["Title"] = "Expense Types";
}

<a asp-area="" asp-controller="ExpenseTypes" asp-action="Add">Add</a>

<table>
  <thead>
    <tr>
      <th scope="col">Expense Type</th>      
    </tr>
  </thead>
  <tbody>
    @foreach (var exp in Model.ExpenseTypes)
    {
      <tr>
        <td>@exp.ExpenseType</td>        
      </tr>

    }

  </tbody>
</table>
